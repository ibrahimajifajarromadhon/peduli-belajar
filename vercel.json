{
    "version": 2,
    "builds": [
      { "src": "public/**/*", "use": "@vercel/static" },
      { "src": "src/**/*.jsx", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/loginAdmin", "dest": "/src/pages/auth/LoginAdmin.jsx" },
      { "src": "/login", "dest": "/src/pages/auth/Login.jsx" },
      { "src": "/register", "dest": "/src/pages/auth/Register.jsx" },
      { "src": "/registerAdmin", "dest": "/src/pages/auth/RegisterAdmin.jsx" },
      { "src": "/reset-password", "dest": "/src/pages/auth/ResetPassword.jsx" },
      { "src": "/otp/:email", "dest": "/src/pages/auth/Otp.jsx" },
      { "src": "/otpAdmin/:email", "dest": "/src/pages/auth/OtpAdmin.jsx" },
      { "src": "/admin/dashboard", "dest": "/src/pages/AdminDashboard.jsx" },
      { "src": "/admin/class", "dest": "/src/pages/ManageClassAdmin.jsx" },
      { "src": "/myClass", "dest": "/src/pages/user/MyClassHomepage.jsx" },
      { "src": "/allCourseClass", "dest": "/src/pages/user/AllCourseHomepage.jsx" },
      { "src": "/bayarCourse", "dest": "/src/pages/user/BayarCourse.jsx" },
      { "src": "/successBayarCourse", "dest": "/src/pages/user/SuccessBayarCourse.jsx" },
      { "src": "/detailClass/:courseCode", "dest": "/src/pages/user/DetailCourse.jsx" },
      { "src": "/userProfile/myProfile", "dest": "/src/components/user/MyProfile.jsx" },
      { "src": "/userProfile/changePassword", "dest": "/src/components/user/ChangePassword.jsx" },
      { "src": "/userProfile/paymentHistory", "dest": "/src/components/user/PaymentHistory.jsx" },
      { "src": "/notification", "dest": "/src/pages/user/Notification.jsx" },
      { "src": "/welcome", "dest": "/src/App.jsx" }
    ]
  }
  